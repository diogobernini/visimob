<Window x:Class="visimob_desktop.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="visimob Desktop" Height="600" Width="1000" Closing="Window_Closing">
    <DockPanel>
        <ToolBarTray DockPanel.Dock="Top">
            <ToolBar>
                <Button Content="Importar Imóveis" Click="abreVistoria" />
                <Button Content="Abrir Imóveis Locais" Click="abreVistoria" />
                <Button Content="Editar Contratos" />
                <Button Content="Configurações" />
                <Button Content="Ajuda" />
            </ToolBar>
        </ToolBarTray>
        <Rectangle Fill="DarkGray"/>
        <Image HorizontalAlignment="Center" VerticalAlignment="Center" Width="500" Source="Images/logo.png"/>
    </DockPanel>
</Window>